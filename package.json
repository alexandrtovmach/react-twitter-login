    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.5",
    "lint-staged": "^9.4.0",
    "prettier": "^1.14.3",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
